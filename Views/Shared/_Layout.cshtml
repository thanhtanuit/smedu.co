    <body style="background:#e8edf2 none repeat scroll 0 0">
        <header>     
            <!--== MAIN CONTRAINER ==-->
            <div class="container-fluid sb1">
                <div class="row">
                    <!--== LOGO ==-->
                    <div class="col-md-2 col-sm-3 col-xs-6 sb1-1">
                        <a href="#" class="btn-close-menu"><i class="fa fa-times" aria-hidden="true"></i></a>
                        <a href="#" class="atab-menu"><i class="fa fa-bars tab-menu" aria-hidden="true"></i></a>
                        <a href="@Url.Action("Index","Administrator")" class="logo"><img src="~/assets/images/logo.png" alt="" /></a>
                    </div>
                    <!--== SEARCH ==-->
                    <div class="col-md-6 col-sm-6 mob-hide">
                        <div class="title-top-admin" style="width:100%; margin:auto; text-align:center; color:#214497; padding-top: 21px;font-size: 20px;"><strong>Hệ Thống Quản Lý Đoàn Viên</strong></div>
                        @*<form class="app-search">
                        <input type="text" placeholder="Search..." class="form-control">
                        <a href=""><i class="fa fa-search"></i></a>
                    </form>*@
                    </div>
                    <!--== NOTIFICATION ==-->
                    <div class="col-md-2 tab-hide" style="float:left; ">
                        @*<a class='waves-effect btn-noti' href='#'><i class="fa fa-commenting-o" aria-hidden="true"></i><span>5</span></a>*@
                        <div class="top-not-cen" style="float:right;  padding:5px; padding-left:10px;">                           
                            <a href="@Url.Action("Index","Administrator")"><img width="50" src="/trangchu/images/logo_doan.png" alt="" /></a>
                        </div>                     
                    </div>
                    <!--== MY ACCCOUNT ==-->
                    <div class="col-md-2 col-sm-3 col-xs-6">

                        <!-- Dropdown Trigger -->
                        <a class='waves-effect dropdown-button top-user-pro' href='#' data-activates='top-menu'>
                            <img src="~/assets/images/user/6.png" alt="" />Tài khoản<i class="fa fa-angle-down" aria-hidden="true"></i>
                        </a>
                    
                        <!-- Dropdown Structure -->
                        <ul id='top-menu' class='dropdown-content top-menu-sty' style="top:56px!important;">
                            <li>
                                <a href="@Url.Content("~/SettingAccount/Index")" class="waves-effect"><i class="fa fa-cogs" aria-hidden="true"></i>Thông tin tài khoản</a>
                            </li>                           
                            <li>
                                <a href="@Url.Content("~/SettingAccount/ChangePassword")" class="waves-effect"><i class="fa fa-undo" aria-hidden="true"></i> Đổi mật khẩu</a>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <a href="@Url.Content("/Account/LogOff")" class="ho-dr-con-last waves-effect"><i class="fa fa-sign-in" aria-hidden="true"></i> Đăng xuất</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>
           
        <div class="container-fluid sb2">
