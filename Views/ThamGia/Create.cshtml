@model Bidc_Union_Management.Models.ThamGia


<div class="sb2-2">
    <div class="sb2-2-2">
        <ul>
            <li><a href="/"><i class="fa fa-home" aria-hidden="true"></i>Home</a>
            </li>
            <li class="active-bre"><a href="#">Tham gia</a>
            </li>
        </ul>
    </div>
    <div class="sb2-2-3">
        <div class="row">
            <div class="col-md-12">
                <div class="box-inn-sp">
                    <div class="inn-title">
                        <h4>Thêm mới nhiệm kỳ</h4>
                    </div>
                    <div class="tab-inn">
                        @using (Html.BeginForm())
                        {
                            @Html.AntiForgeryToken()
                            @Html.ValidationSummary(true)
                            <div class="row">
                                <div class="input-field col s12">
                                    <div class="col-md-10">
                                        <strong>Hoạt động:<br />
                                        </strong>
                                        @Html.DropDownList("hoatdongid",new SelectList(ViewBag.listhoatdongs,"Value","Text"))
                                    </div>
                                </div>
                                <div class="input-field col s12">
                                    <div class="col-md-10">
                                        <strong>Thành viên:<br />
                                        </strong>
                                        @Html.DropDownList("doanvienid",new SelectList(ViewBag.listdoanviens,"Value","Text"))
                                    </div>
                                </div>
                                <div class="input-field col s12">
                                    <div class="col-md-10">
                                        <input type="submit" value="Lưu" class="btn-large">
                                    </div>
                                </div>
                            </div>                                      
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
